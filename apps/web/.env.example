# ===========================================
# GK-Nexus Web Frontend Configuration
# ===========================================
# Copy this file to .env and fill in your values

# ----- API Server -----
# Backend API endpoint URL
# Development: http://localhost:3000
# Production: https://api.yourdomain.com or https://yourdomain.com
VITE_SERVER_URL=

# ----- Application Settings (Optional) -----
# Application name displayed in UI
# VITE_APP_NAME=GK-Nexus

# Application version (auto-populated from package.json in build)
# VITE_APP_VERSION=

# ----- Feature Flags (Optional) -----
# Enable/disable features during development
# VITE_ENABLE_CLIENT_PORTAL=true
# VITE_ENABLE_WHATSAPP=false
# VITE_ENABLE_INVOICING=true

# ----- Analytics (Optional) -----
# Google Analytics tracking ID
# VITE_GA_TRACKING_ID=

# ----- Debugging (Optional) -----
# Enable debug logging in development
# VITE_DEBUG=false
